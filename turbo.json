{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "test:cov": {
      "dependsOn": ["build"],
      "cache": false,
      "outputs": ["coverage/**"]
    },
    "lint": {},
    "lint:fix": {
      "cache": false
    },
    "format": {},
    "format:fix": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
